{
  "sdk": 3,
  "version": 10000,
  "name": "MSFS Touch Portal Plugin",
  "id": "MSFSTouchPortalPlugin",
  "configuration": {
    "colorDark": "#212121",
    "colorLight": "#0090ff"
  },
